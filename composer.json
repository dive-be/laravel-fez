{
    "name": "dive-be/laravel-fez",
    "description": "Zero config document head management",
    "keywords": [
        "dive",
        "laravel",
        "fez",
        "seo",
        "head",
        "open",
        "graph",
        "facebook",
        "twitter",
        "og"
    ],
    "homepage": "https://github.com/dive-be/laravel-fez",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammed Sari",
            "email": "muhammed@dive.be",
            "homepage": "https://dive.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "dive-be/php-utils": "^0.1.0",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/larastan": "^2.1",
        "orchestra/testbench":"^7.5",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Dive\\Fez\\": "src",
            "Dive\\Fez\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/php-cs-fixer fix --config .php-cs-fixer.dist.php --allow-risky=yes",
        "test": "vendor/bin/pest",
        "verify": "@composer analyse && composer test"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dive\\Fez\\FezServiceProvider"
            ],
            "aliases": {
                "Fez": "Dive\\Fez\\Facades\\Fez"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
